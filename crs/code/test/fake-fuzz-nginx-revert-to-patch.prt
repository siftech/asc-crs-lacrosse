  (push :target *debug-list*)
  (setf *run-fake-bridge* t)
  (setf *fake-fuzz* t)
  (setf *fake-git-bisect* t)
  (setf *lacrosse* t)
  (setf *use-revert-to-patch-p* t)
  (setf *revert-to-patch-limit-per-bic* 70)
  (setf *use-lacrosse-llm-gen-patch-p* nil)
  (setf *fake-bridge-script*
    \'(
       (1 ((:type :new-challenge-project)
           (:target ((;;(:cp-address "")
                        ;;(:cp-address "")
				)))))
;;       (12000 ((:type :halt)))
))
';

our $numfbs = 2;
our $allocationmethod = "balanced";

our $cp_config = "../crs-sandbox-items/nginx-cp-config.yaml";
our $success_pattern = "Patch status received: accepted, CPV_UUID=";
