do "./defaults.pl" || die "Can't do './defaults.pl': $@";

$run_in_compose = 1;

our $experiment = '
  (push :msg *debug-list*)
  (push :amp *debug-list*)
  (push :target *debug-list*)
  (setf *run-fake-bridge* t)
  (setf *fake-fuzz* nil)
  (setf *lacrosse* t)
  (setf *fake-bridge-script*
    \'(
       (1 ((:type :new-challenge-project)
           (:target ((;;(:cp-address "")
                        ;;(:cp-address "")
				)))))
;;       (12000 ((:type :halt)))
))
';

our $numfbs = 1;
