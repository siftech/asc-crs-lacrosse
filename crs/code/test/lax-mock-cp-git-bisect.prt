do "./common.pl" || die "Can't do './common.pl': $@";

$test = {
    defaultIgnore => 1,	# subset of try-patch
    keywords => [ qw / lacrosse / ],
    cleanup => \&condor_killer,
    agents => [
        @clean_and_start_agents,
    ],
};

$test;

